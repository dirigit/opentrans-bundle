{
    "name": "se/opentrans-bundle",
    "type": "bundle",
    "description": "This bundle integrates the opentrans library into Symfony2. Allows to create OpenTRANS compatible documents from PHP.",
    "keywords": ["symfony2", "bundle", "opentrans", "document", "serialization", "xml"],
    "homepage": "https://github.com/sveneisenschmidt/opentrans-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Eisenschmidt",
            "email": "sven.eisenschmidt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "se/opentrans": "0.1.*@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "SE\\Component\\OpenTrans\\Tests": "tests/",
            "SE\\Component\\OpenTrans": "src/"
        }
    }
}